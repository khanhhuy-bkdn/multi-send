{
  "name": "multi-send-contract",
  "version": "1.0.0",
  "description": "Smart contract for sending tokens to multiple addresses on BSC",
  "main": "index.js",
  "scripts": {
    "compile": "hardhat compile",
    "test": "hardhat test",
    "deploy:bsc-testnet": "hardhat run scripts/deploy.ts --network bsc-testnet",
    "deploy:bsc-mainnet": "hardhat run scripts/deploy.ts --network bsc-mainnet",
    "deploy:bsc-ultra-low": "hardhat run scripts/deploy.ts --network bsc-ultra-low",
    "example:bsc-testnet": "hardhat run scripts/example-multisend.ts --network bsc-testnet",
    "example:bsc-mainnet": "hardhat run scripts/example-multisend.ts --network bsc-mainnet",
    "example:bsc-ultra-low": "hardhat run scripts/example-multisend.ts --network bsc-ultra-low",
    "gas-estimate": "hardhat run scripts/gas-estimation.ts --network hardhat",
    "verify": "hardhat verify --network bsc-mainnet",
    "typechain": "hardhat typechain"
  },
  "keywords": [
    "blockchain",
    "bsc",
    "multi-send",
    "token",
    "batch"
  ],
  "author": "Blockchain Developer",
  "license": "MIT",
  "devDependencies": {
    "@nomicfoundation/hardhat-toolbox": "^4.0.0",
    "@openzeppelin/contracts": "^5.0.0",
    "@typechain/ethers-v6": "^0.5.1",
    "@typechain/hardhat": "^9.1.0",
    "@types/chai": "^5.2.2",
    "@types/mocha": "^10.0.10",
    "@types/node": "^24.7.0",
    "chai": "^4.3.7",
    "ethers": "^6.8.0",
    "hardhat": "^2.19.0",
    "ts-node": "^10.9.2",
    "typechain": "^8.3.2",
    "typescript": "^5.9.3"
  },
  "dependencies": {
    "dotenv": "^16.3.1"
  }
}